each branch in ['master', 'staging', 'develop']
  div.row.pr
    div.span6
      a(href='https://github.com/coursefork/forkshop') #{branch}

    div.offset1.span2
      - if (prs[ branch ] && prs[ branch ].on_staging)
        button.btn.btn-success On Staging
      - else
        button.btn(data-id='#{branch}') Checkout
