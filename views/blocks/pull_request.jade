div.row.pr
  div.span6
    ##{pull_request.number}. 
    a(href='#{pull_request.html_url}') #{pull_request.title}

  div.offset1.span2
    - if (prs[ "pr/" + pull_request.number ] && prs[ "pr/" + pull_request.number ].on_staging)
      button.btn.btn-success On Staging
    - else
      button.btn(data-id='pr/#{pull_request.number}') Checkout
